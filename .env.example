CLAUDE_SESSION_KEY=sk-ant-sid01-YOUR_SESSION_KEY_HERE-AoFuUAAA
CLAUDE_ORG_ID=17fcb197-98f1-4c44-9ed8-bc89b419cbbf
CLAUDE_POLL_INTERVAL=300
CLAUDE_MAX_HISTORY_DAYS=7

