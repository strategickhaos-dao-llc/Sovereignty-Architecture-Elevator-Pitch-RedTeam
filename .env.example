# GitLens Discord Integration Environment Variables
# Copy to .env and fill in your actual values

# Discord Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_GUILD_ID=your_server_id_here

# Discord Channel IDs (enable Developer Mode in Discord to get these)
PRS_CHANNEL=1234567890123456789
DEV_FEED_CHANNEL=1234567890123456789
DEPLOYMENTS_CHANNEL=1234567890123456789
CLUSTER_STATUS_CHANNEL=1234567890123456789
ALERTS_CHANNEL=1234567890123456789
AGENTS_CHANNEL=1234567890123456789

# GitHub Integration
GITHUB_ORG=your-github-org
GITHUB_APP_ID=123456
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# GitLens Configuration
GITLENS_EDITION=pro
VSCODE_USERS_GROUP=devs

# Event Gateway
EVENT_GATEWAY_URL=https://events.yourco.com
HMAC_SECRET=your_hmac_secret

# Environment
NODE_ENV=development
DEBUG=false

# ============================================================
# AI Council Orchestrator Configuration
# ============================================================

# LLM Provider API Keys (at least one required)
# X.AI / Grok
XAI_API_KEY=your_xai_api_key_here
XAI_KEY=your_xai_api_key_here

# Anthropic / Claude
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_KEY=your_anthropic_api_key_here

# OpenAI / GPT-4
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_KEY=your_openai_api_key_here

# Google / Gemini
GOOGLE_API_KEY=your_google_api_key_here
GOOGLE_KEY=your_google_api_key_here

# Council Notifications (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/your_webhook_here
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/your_webhook_here

# Grafana (for council monitoring)
GRAFANA_PASSWORD=admin