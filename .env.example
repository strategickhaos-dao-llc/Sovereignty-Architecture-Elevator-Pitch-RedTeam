# GitLens Discord Integration Environment Variables
# Copy to .env and fill in your actual values

# Discord Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_GUILD_ID=your_server_id_here

# Discord Channel IDs (enable Developer Mode in Discord to get these)
PRS_CHANNEL=1234567890123456789
DEV_FEED_CHANNEL=1234567890123456789
DEPLOYMENTS_CHANNEL=1234567890123456789
CLUSTER_STATUS_CHANNEL=1234567890123456789
ALERTS_CHANNEL=1234567890123456789
AGENTS_CHANNEL=1234567890123456789

# GitHub Integration
GITHUB_ORG=your-github-org
GITHUB_APP_ID=123456
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# GitLens Configuration
GITLENS_EDITION=pro
VSCODE_USERS_GROUP=devs

# Event Gateway
EVENT_GATEWAY_URL=https://events.yourco.com
HMAC_SECRET=your_hmac_secret

# Environment
NODE_ENV=development
DEBUG=false
REFINORY_ENV=development

# Grafana Cloud - AI-Powered Observability
# Generate API token at: https://grafana.com/profile/api-keys
GRAFANA_CLOUD_API_TOKEN=your_grafana_cloud_api_token

# Grafana Cloud Prometheus (Metrics)
GRAFANA_CLOUD_PROMETHEUS_URL=https://prometheus-prod-56-prod-us-east-2.grafana.net/api/prom/push
GRAFANA_CLOUD_PROMETHEUS_USER=2786173
GRAFANA_CLOUD_PROMETHEUS_QUERY_URL=https://prometheus-prod-56-prod-us-east-2.grafana.net/api/prom

# Grafana Cloud Loki (Logs)
GRAFANA_CLOUD_LOKI_URL=https://logs-prod-012.grafana.net/loki/api/v1/push
GRAFANA_CLOUD_LOKI_USER=123456

# Grafana Cloud Tempo (Traces)
GRAFANA_CLOUD_TEMPO_URL=https://tempo-prod-10-prod-us-east-0.grafana.net:443
GRAFANA_CLOUD_TEMPO_USER=123456

# Grafana Cloud Instance (for dashboards and visualization)
GRAFANA_CLOUD_INSTANCE_URL=https://me1010101.grafana.net
GRAFANA_CLOUD_ORG_ID=me1010101