# GitLens Discord Integration Environment Variables
# Copy to .env and fill in your actual values

# Discord Configuration
DISCORD_TOKEN=your_bot_token_here
DISCORD_GUILD_ID=your_server_id_here

# Discord Channel IDs (enable Developer Mode in Discord to get these)
PRS_CHANNEL=1234567890123456789
DEV_FEED_CHANNEL=1234567890123456789
DEPLOYMENTS_CHANNEL=1234567890123456789
CLUSTER_STATUS_CHANNEL=1234567890123456789
ALERTS_CHANNEL=1234567890123456789
AGENTS_CHANNEL=1234567890123456789

# Research & Patent Discord Channels
CH_PATENT_RESEARCH_ID=1234567890123456789
CH_PATENT_STATUS_ID=1234567890123456789
CH_RESEARCH_FEED_ID=1234567890123456789
CH_RESEARCH_CITATIONS_ID=1234567890123456789

# GitHub Integration
GITHUB_ORG=your-github-org
GITHUB_APP_ID=123456
GITHUB_WEBHOOK_SECRET=your_webhook_secret

# GitLens Configuration
GITLENS_EDITION=pro
VSCODE_USERS_GROUP=devs

# Event Gateway
EVENT_GATEWAY_URL=https://events.yourco.com
HMAC_SECRET=your_hmac_secret

# Patent Office APIs
USPTO_API_KEY=your_uspto_api_key
EPO_API_KEY=your_epo_api_key

# Google Scholar / Research APIs
SERPAPI_KEY=your_serpapi_key

# Database
POSTGRES_PASSWORD=dev_password

# AI Services
OPENAI_API_KEY=your_openai_api_key

# GPG Configuration
GPG_KEY_ID=0x137SOVEREIGN

# Environment
NODE_ENV=development
DEBUG=false