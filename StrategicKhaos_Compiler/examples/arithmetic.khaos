; arithmetic.khaos - Demonstrating arithmetic operations
; Test basic arithmetic in the StrategicKhaos language

; Addition
(print "2 + 3 =")
(print (+ 2 3))

; Subtraction
(print "10 - 4 =")
(print (- 10 4))

; Multiplication
(print "6 * 7 =")
(print (* 6 7))

; Division
(print "20 / 4 =")
(print (/ 20 4))

; Nested expressions
(print "Nested: (2 + 3) * 4 =")
(print (* (+ 2 3) 4))

; Multiple arguments
(print "Sum of 1, 2, 3, 4 =")
(print (+ 1 2 3 4))
